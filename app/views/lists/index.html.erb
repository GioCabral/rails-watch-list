<div class="cont">
<div class="header" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('data:image/jpeg;base64,')">
  <h1>TELA DE LISTA</h1>
</div>
<div class="container">
    <div class="row">
    <% @lists.each do |list| %>
        <div class="col-lg-4">

    <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(<%=list.image.url%>)">
      <div class="card-list">
        <h2><%= list.name %></h2>
        <p><%= link_to "Detalhe da lista", list_path(list), class: "btn btn-primary" %></p>
      </div>
    </div>
        </div>
    <% end %>
  </div>
</div>
<%= link_to new_list_path, class:"btn-icon" do  %>
  <div class="btn-new">
    <i class="fa-solid fa-plus"></i><p>Adicionar</p>
  </div>
<% end %>
</div>
